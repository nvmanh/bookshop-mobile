{
  "name": "BookShopMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "start:theme": "node scripts/custom-rn-theme && react-native start",
    "restart": "node scripts/custom-rn-theme && node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "start:8082": "node scripts/custom-rn-theme && node node_modules/react-native/local-cli/cli.js start --port 8082",
    "start:packager": "yarn react-native start --config packager/config.js",
    "tsc": "tsc",
    "test": "jest",
    "lint": "eslint .",
    "lint:ts": "eslint . src/**/*.ts src/**/*.tsx",
    "tslint": "tslint --project . src/**/*.ts src/**/*.tsx",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/YourAppName/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'"
  },
  "dependencies": {
    "@ant-design/icons-react-native": "^2.0.0",
    "@ant-design/react-native": "^3.3.0",
    "@react-native-community/async-storage": "^1.8.1",
    "axios": "^0.19.2",
    "immutable": "^4.0.0-rc.12",
    "iron-redux": "^0.2.7",
    "lodash": "^4.17.15",
    "moment": "^2.29.2",
    "patch-package": "^6.2.1",
    "prop-types": "^15.7.2",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-actionsheet": "^2.4.2",
    "react-native-button": "^2.4.0",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-image-crop-picker": "^0.28.0",
    "react-native-reanimated": "^1.7.0",
    "react-native-router-flux": "^4.2.0",
    "react-native-screens": "^2.3.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.3.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^24.0.24",
    "@types/lodash": "^4.14.149",
    "@types/react-native": "^0.60.25",
    "@types/react-native-actionsheet": "^2.4.1",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "16.9.1",
    "@types/redux-actions": "^2.6.1",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-saga": "^0.10.5",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-root-import": "^6.4.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^6.5.1",
    "eslint-config-alloy": "^3.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "16.9.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.7.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
